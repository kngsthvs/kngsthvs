{
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"@/*": ["./src/*"]
		},
		"plugins": [
			{
				"name": "next"
			}
		],
		"strictNullChecks": true,
		"tsBuildInfoFile": "node_modules/.cache/tsbuildinfo.json"
	},
	"exclude": ["node_modules"],
	"extends": "@kngsthvs/config/tsconfig/nextjs",
	"include": [
		"next-env.d.ts",
		"next.config.mjs",
		"**/*.js",
		"**/*.ts",
		"**/*.tsx",
		".next/types/**/*.ts"
	]
}
